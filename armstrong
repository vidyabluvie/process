import java.io.*;
public class armstrong
{
public static void main(String args[])
{
DataInputStream sys=new DataInputStream(System.in);
int c=0,a,temp;
int n=sys.nextInt();
temp=n;
while(n>0 && n<=100000)
{
a=n%10;
n=n/10;
c=c+(a*a*a);
}
if(temp==c)
{
System.out.println("Yes");
}
else
{
System.out.println("No");
}
}
}
